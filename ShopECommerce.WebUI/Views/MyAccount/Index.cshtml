@model DetailUserViewModel

@{
    ViewData["Title"] = "Kullanıcı Bilgileri";
    Layout = "~/Views/CustomerLayout/Index.cshtml";
}

<div class="right_col" role="main">
    <div class="">
        <div class="page-title">
            <div class="title_left">
                <h3>Müşteri Paneli</h3>
            </div>

            <div class="title_right">
                <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                    <div class="input-group">
                        <input type="text" class="form-control" placeholder="aranacak kelimeleri giriniz...">
                        <span class="input-group-btn">
                            <button class="btn btn-default" type="button">Ara!</button>
                        </span>
                    </div>
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <h1><center>Hoşgeldiniz @Context.User.Identity.Name</center></h1>
                    <br />
                    <h3><center>Menüden işlemlerinizi gerçekleştirebilirsiniz.</center></h3>
                </div>
            </div>
        </div>
    </div>
</div>



